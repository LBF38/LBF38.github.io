{
	"name": "lbf38.github.io",
	"version": "1.0.2",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm i18n && pnpm build:content && vite build",
		"build:content": "contentlayer build",
		"build:content-cachebust": "contentlayer build --clearCache",
		"bump": "pnpm version-packages",
		"changeset": "npx changeset",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "concurrently --kill-others \"pnpm dev:content\" \"pnpm dev:svelte\"",
		"dev:content": "contentlayer dev",
		"dev:svelte": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"gh-pages": "pnpm build && pnpx gh-pages -d build",
		"i18n": "paraglide-js compile --project ./project.inlang",
		"i18n:translate": "pnpx @inlang/cli machine translate --project ./project.inlang",
		"i18n:watch": "pnpm i18n --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"release": "pnpm build && changeset publish",
		"sort": "sort-package-json",
		"test": "npm run test:integration && npm run test:unit",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"version-packages": "changeset version"
	},
	"dependencies": {
		"@changesets/cli": "^2.29.2",
		"@inlang/paraglide-sveltekit": "0.10.6",
		"bits-ui": "^0.21.16",
		"changesets-changelog-clean": "^1.3.0",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.6.0",
		"lucide-svelte": "^0.395.0",
		"mode-watcher": "^0.3.1",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.2.1"
	},
	"devDependencies": {
		"@huntabyte/mdsvex": "^0.16.8",
		"@iconify/svelte": "^4.2.0",
		"@inlang/paraglide-js": "1.10.1",
		"@internationalized/date": "^3.8.0",
		"@octokit/rest": "^20.1.2",
		"@playwright/test": "^1.52.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@sveltejs/vite-plugin-svelte-inspector": "^2.1.0",
		"@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
		"@tailwindcss/typography": "^0.5.16",
		"@types/lodash-es": "^4.17.12",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.21",
		"concurrently": "^8.2.2",
		"contentlayer": "^0.3.4",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.46.1",
		"gh-pages": "^5.0.0",
		"hast-util-to-html": "^9.0.5",
		"lodash-es": "^4.17.21",
		"mdsvex": "^0.11.2",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-pretty-code": "^0.13.2",
		"rehype-rewrite": "^4.0.2",
		"rehype-slug": "^6.0.0",
		"remark-abbr": "^1.4.2",
		"remark-code-import": "^1.2.0",
		"remark-gfm": "^4.0.1",
		"remark-github": "^12.0.0",
		"shiki": "^1.29.2",
		"sort-package-json": "^2.15.1",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"svelte-seo": "^1.6.1",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0",
		"vite": "^5.4.18",
		"vite-plugin-kit-routes": "^0.6.11",
		"vitest": "^0.25.8"
	}
}
